	ApplyAnimation(PLAYER_TEMP[playerid][pt_MECHANIC_PID], "BOMBER", "BOM_Plant", 4.1, false, true, true, false, 3000);
    return 1;
}
CMD:dararma(playerid, params[])
{
    new to_player, arma, balas;
	if(sscanf(params, "udd", to_player, arma, balas)) return SendClientMessage(playerid, -1, "Uso: /dararma [ID] [ID da arma] [Munição]");
	if(!IsPlayerConnected(to_player)) return SendClientMessageEx(playerid, -1, "Jogador (%d) desconectado", to_player);
	GivePlayerWeapon(playerid, arma, balas);
	
	SendCmdLogToAdmins(playerid, "dararma", params);
	return 1;
}


