		if(PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_EXTRA] > PLAYER_MISC[playerid][MISC_CRACK]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
