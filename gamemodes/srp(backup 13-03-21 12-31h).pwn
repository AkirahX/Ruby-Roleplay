#define TIMER_CONSUMIR
CMD:consumir(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_CRACK || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Não pode usar esse comando agora.");
	new option[24], slot;
	if(!sscanf(params, "s[24]d", option, slot))	
	{
		if(!strcmp(option, "alimento", true))
		{
			if(slot < 1 || slot > MAX_PLAYER_POCKET_OBJECTS) return SendClientMessageEx(playerid, -1, "Erro: {C4FF66}/consumir alimento {FFFFFF}[SLOT 1-%d]", MAX_PLAYER_POCKET_OBJECTS);
		
			slot --;
			
			if(!PLAYER_POCKET[playerid][slot][player_pocket_VALID]) return SendClientMessage(playerid, -1, "{999999}Não tem nada nesse slot.");
			if(CHARACTER_INFO[playerid][ch_HUNGRY] >= 99.0 && CHARACTER_INFO[playerid][ch_THIRST] >= 99.0) return SendClientMessage(playerid, -1, "{CCCCCC}Ta querendo vomitar?");

			Add_Hungry_Thirst(playerid, PLAYER_POCKET[playerid][slot][player_pocket_object_HUNGRY], PLAYER_POCKET[playerid][slot][player_pocket_object_THIRST]);
			GivePlayerDrunkLevel(playerid, PLAYER_POCKET[playerid][slot][player_pocket_object_DRUNK]);
			
			new action[64];
			format(action, sizeof action, "consome %s.", PLAYER_POCKET[playerid][slot][player_pocket_object_NAME]);
			Auto_SendPlayerAction(playerid, action);
			
			DeletePlayerPocketObject(playerid, slot);

			ApplyAnimation(playerid, "FOOD", "EAT_Pizza", 0, 0, 0, 0, 0, 0);
			ApplyAnimation(playerid, "FOOD", "EAT_Pizza", 4.1, false, true, true, false, 1000);
		}
		else SendClientMessage(playerid, -1, "Erro: {C4FF66}/consumir {FFFFFF}[medicamento - maconha - cocaina - alimento]");
	}
	else if(!sscanf(params, "s[24]", option))
	{
		if(!strcmp(option, "medicamento", true))
		{
			if(PLAYER_MISC[playerid][MISC_MEDICINE] <= 0) return SendClientMessage(playerid, -1, "{999999}Você não tem medicamentos.");
			if(CombatInfo[playerid][cMc] >=1) return SendClientMessage(playerid, -1, "{999999}Não pode consumir durante um combate.");
			if(TimerInfo[playerid][tMed] >= 1) return SendClientMessage(playerid, -1, "{999999}Você não pode consumir medicamento tão rápido");
			
			PLAYER_MISC[playerid][MISC_MEDICINE] --;
			GivePlayerHealthEx(playerid, 25.0);
			Auto_SendPlayerAction(playerid, "consome um medicamento.");
			ApplyAnimation(playerid, "FOOD", "EAT_Burger", 0, 0, 0, 0, 0, 0);
			ApplyAnimation(playerid, "FOOD", "EAT_Burger", 3.00, false, true, true, false, 3000);
			TimerInfo[playerid][tMed]++;
			SetTimerEx("TimerCon", 60000, false, "i", playerid);
		}
		else if(!strcmp(option, "maconha", true))
		{
			if(PLAYER_MISC[playerid][MISC_CANNABIS] <= 0) return SendClientMessage(playerid, -1, "{999999}Você não tem maconha.");
			if(CombatInfo[playerid][cMc] >=1) return ShowPlayerMessage(playerid, "Modo combate", 3);
			if(TimerInfo[playerid][tMaconha] >= 1) return SendClientMessage(playerid, -1, "{999999}Você não pode fumar tanta maconha rapidamente.");
			
			PLAYER_MISC[playerid][MISC_CANNABIS] --;
			GivePlayerHealthEx(playerid, 30.0);
			Auto_SendPlayerAction(playerid, "fuma um cigarro maconha.");
			GivePlayerDrunkLevel(playerid, 1000);
			TimerInfo[playerid][tMaconha]++;
			SetTimerEx("TimerCon", 60000, false, "i", playerid);
		}
		else if(!strcmp(option, "cocaina", true))
		{
			if(PLAYER_MISC[playerid][MISC_CRACK] <= 0) return SendClientMessage(playerid, -1, "{999999}Você não tem cocaína.");
			if(CombatInfo[playerid][cMc] >=1) return ShowPlayerMessage(playerid, "Modo combate", 3);
			if(TimerInfo[playerid][tCoca] >= 1) return SendClientMessage(playerid, -1, "{999999}Você não pode consumir cocaina tão rápido.");
			
			PLAYER_MISC[playerid][MISC_CRACK] --;
			GivePlayerArmourEx(playerid, 20.0);
			Auto_SendPlayerAction(playerid, "consome cocaina.");
			ApplyAnimation(playerid, "FOOD", "EAT_Pizza", 0, 0, 0, 0, 0, 0);
			ApplyAnimation(playerid, "FOOD", "EAT_Pizza", 4.1, false, true, true, false, 1000);
			GivePlayerDrunkLevel(playerid, 2000);
			TimerInfo[playerid][tCoca]++;
			SetTimerEx("TimerCon", 60000, false, "i", playerid);
		}
		else if(!strcmp(option, "alimento", true)) SendClientMessageEx(playerid, -1, "Erro: {C4FF66}/consumir alimento {FFFFFF}[SLOT 1-%d]", MAX_PLAYER_POCKET_OBJECTS);
		else SendClientMessage(playerid, -1, "Erro: {C4FF66}/consumir {FFFFFF}[medicamento - maconha - cocaina - alimento]");
	}
	else SendClientMessage(playerid, -1, "Erro: {C4FF66}/consumir {FFFFFF}[medicamento - maconha - cocaina - alimento]");
	return 1;
}
