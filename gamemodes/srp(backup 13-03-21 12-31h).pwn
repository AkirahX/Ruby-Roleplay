CMD:mutar(playerid, params[])
{
    new to_player, reason[128], time;
    if(sscanf(params, "uds[128]", to_player, time, reason)) return SendClientMessage(playerid, -1, "Syntax: /mutar <player_id> <minutos> <razão>");
	if(time < 0 || time > 1440) return SendClientMessage(playerid, -1, "Intervalo de minutos incorreto.");
    if(!IsPlayerConnected(to_player)) return SendClientMessageEx(playerid, -1, "Jogador (%d) desconectado", to_player);
    if(ACCOUNT_INFO[to_player][ac_ADMIN_LEVEL] > ACCOUNT_INFO[playerid][ac_ADMIN_LEVEL]) return SendClientMessage(playerid, -1, "O cargo administrativo desse jogador é superior ao seu.");

	new seconds = time * 60;
	SendClientMessageEx(to_player, -1, "Você foi silenciado do canal da dúvida por %d minutos, razão: %s", time, reason);
	PLAYER_MISC[to_player][MISC_MUTE] = gettime() + seconds;
	SavePlayerMisc(to_player);
	
	new str[145]; format(str, 145, "{C4FF66}%s (%d) {999999}silenciou {C4FF66} %s (%d) {999999}do canal de dúvidas: %s", ACCOUNT_INFO[playerid][ac_NAME], playerid, ACCOUNT_INFO[to_player][ac_NAME], to_player, reason);
    SendAdminAd(-1, str);
	return 1;
}

CMD:desmutar(playerid, params[])
{
    new to_player;
    if(sscanf(params, "u", to_player)) return SendClientMessage(playerid, -1, "Syntax: /desmuteard <player_id>");
    if(!IsPlayerConnected(to_player)) return SendClientMessageEx(playerid, -1, "Jogador (%d) desconectado", to_player);
    if(ACCOUNT_INFO[to_player][ac_ADMIN_LEVEL] > ACCOUNT_INFO[playerid][ac_ADMIN_LEVEL]) return SendClientMessage(playerid, -1, "O cargo administrativo desse jogador é superior ao seu.");

	if(gettime() > PLAYER_MISC[to_player][MISC_MUTE]) return SendClientMessage(playerid, -1, "Este jogador não está silenciado.");
	
	SendClientMessage(to_player, -1, "Você já pode enviar dúvidas.");
	PLAYER_MISC[to_player][MISC_MUTE] = 0;
	SavePlayerMisc(to_player);
	
	SendClientMessageEx(playerid, -1, "Jogador %s (%d) foi des-silenciado.", ACCOUNT_INFO[to_player][ac_NAME], to_player);
	return 1;
}

