		SendClientMessageEx(playerid, -1, "{CCCCCC}Você ofertou algo para %s, aguarde que o outro jogador aceite.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		ShowDialog(to_playerid, DIALOG_TRICKS_MEDICINE);
		return 1;
	}
	else if(!strcmp(option, "maconha", true))
	{
		if(PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_EXTRA] <= 0 || PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_EXTRA] > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade insuficiente.");
		if(PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_EXTRA] > PLAYER_MISC[playerid][MISC_CANNABIS]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
