		}


		else if(IsPlayerInRangeOfPoint(playerid, 11.0, 1331.6516,1466.8707,10.8203)) // LV
 		{
	    	if(CHARACTER_INFO[playerid][ch_CASH] >= 750)
			{
				GivePlayerCash(playerid, -750);
            	RepairVehicleEx(vehicleid, playerid);
				SendClientMessage(playerid, -1, "{CCCCCC}Seu veículo foi reparado por {999999}$750.");
			}
			else SendClientMessage(playerid, -1, "{999999}Você não tem dinheiro suficiente.");

		}

		else ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
        
	}
	else SendClientMessage(playerid, -1, "{999999}Você não está em um veículo aéreo.");
	return 1;
}



CMD:janelas(playerid, params[])
{
    if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, 0xFFFFFFAA, "Você não está em um veículo.");

	new options[24];
	if(sscanf(params, "s[24]", options)) return SendClientMessage(playerid, -1, "{999999}Erro, uso correto do comando {CCCCCC}janelas [abrir - fechar].");

	if(!strcmp(options, "fechar", true))
	{
    	SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 1, 1, 1, 1); 
		Auto_SendPlayerAction(playerid, "fechou as janelas de seu veículo.");
	}

	if(!strcmp(options, "abrir", true))
	{
    	SetVehicleParamsCarWindows(GetPlayerVehicleID(playerid), 0, 0, 0, 0); 
		Auto_SendPlayerAction(playerid, "abriu as janelas de seu veículo.");
	}
	return 1;
}
