CMD:soltar(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}No momento não pode usar este comando.");
	new option[24], extra;
	if(sscanf(params, "s[24]d", option, extra)) return SendClientMessage(playerid, -1, "{999999}Erro nos parâmetros, use {CCCCCC}/manual soltar.");
	
