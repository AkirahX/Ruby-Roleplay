	if(!PLAYER_PHONE[playerid][player_phone_VALID])
	{
		ShowPlayerMessage(playerid, "Você nao tem um celular, pode comprar um em qualquer 24/7.", 3, 1085);
		return 1;
	}
	
	if(PLAYER_PHONE[playerid][player_phone_STATE] == PHONE_STATE_OFF) return SendClientMessageEx(playerid, -1, "{999999}Seu celular esta desligado, para ligar use /movil.");
	ShowPlayerShop(playerid);
	return 1;
}

CMD:chamar(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_CRACK || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Não pode usar esse comando agora.");
	if(!PLAYER_PHONE[playerid][player_phone_VALID])
	{
		ShowPlayerMessage(playerid, "Você nao tem um celular, pode comprar um em qualquer 24/7.", 3, 1085);
		return 1;
	}
	if(PLAYER_PHONE[playerid][player_phone_STATE] == PHONE_STATE_OFF) return SendClientMessageEx(playerid, -1, "{999999}Seu celular esta desligado, para ligar use /movil.");
	if(PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL]) return SendClientMessageEx(playerid, -1, "{999999}Você está em uma chamada.");
	
	new call_number, tmp_number, tmp_contact[24], bool:found;
	if(!sscanf(params, "d", tmp_number))
	{
		found = true;
		call_number = tmp_number;
	}
	else if(!sscanf(params, "s[24]", tmp_contact))
	{
		// con strcmp daba crash por las tilder
		new DBResult:Result, DB_Query[145];
		format(DB_Query, sizeof DB_Query, "SELECT `PHONE_NUMBER` FROM `PHONE_BOOK` WHERE `PHONE_ID` = '%d' AND `NAME` LIKE '%%%q%%' LIMIT 1;", PLAYER_PHONE[playerid][player_phone_NUMBER], tmp_contact);
		Result = db_query(Database, DB_Query);
		
		if(db_num_rows(Result))
		{
			found = true;
			call_number = db_get_field_int(Result, 0);
		}
		db_free_result(Result);
	}	
	else return SendClientMessage(playerid, -1, "Erro: {C4FF66}/chamar {FFFFFF}[Número do contato da /agenda]");
	
	if(!found) return SendClientMessageEx(playerid, -1, "{999999}Não foi encontrado o '%s' em sua /agenda.", tmp_contact);
	if(call_number <= 0) return SendClientMessage(playerid, -1, "{999999}O número do destinatário é inválido.");

	switch(call_number)
	{
		case 911:
		{
			if(ACCOUNT_INFO[playerid][ac_LEVEL] < 2) return SendClientMessage(playerid, -1, "{999999}Necessário ser nível 2 para ligar para polícia.");
			PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL] = true;
			PLAYER_TEMP[playerid][pt_POLICE_CALL_NAME] = true;
			PLAYER_TEMP[playerid][pt_POLICE_CALL_DESCRIPTION] = false;
			
			SendClientMessage(playerid, -1, "{CCCCCC}Operadora: Você ligou para central de policía, informe seu nome por favor.");
			return 1;
		}
		case 450: //Taxi
		{
			if(PLAYER_TEMP[playerid][pt_WANT_TAXI])
			{
				PLAYER_TEMP[playerid][pt_WANT_TAXI] = false;
				DisablePlayerTaxiMark(playerid);
				SendClientMessage(playerid, -1, "{CCCCCC}O táxi foi cancelado.");
			}
			else
			{
				PLAYER_TEMP[playerid][pt_WANT_TAXI] = true;
				SendClientMessage(playerid, -1, "{CCCCCC}Sua chamada foi enviada aos taxistas disponíveis, aguarde.");
				SendClientMessage(playerid, -1, "{CCCCCC}Ligue para esse número novamente se você não precisar mais de um táxi.");
				SendAlertToTaxiDrivers(playerid);
			}
			return 1;
		}
		case 570: //Mecanico
		{
			if(PLAYER_TEMP[playerid][pt_WANT_MECHANIC])
			{
				PLAYER_TEMP[playerid][pt_WANT_MECHANIC] = false;
				DisablePlayerMechanicMark(playerid);
				SendClientMessage(playerid, -1, "{CCCCCC}O mecânico foi cancelado.");
			}
			else
			{
				if(!IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, -1, "{999999}Você precisa esta dentro do veículo avariado para solicitar o mecânico.");
				if(GLOBAL_VEHICLES[ GetPlayerVehicleID(playerid) ][gb_vehicle_STATE] != VEHICLE_STATE_DAMAGED) return SendClientMessage(playerid, -1, "{999999}Você precisa esta dentro do veículo avariado para solicitar o mecânico.");
				
				PLAYER_TEMP[playerid][pt_WANT_MECHANIC] = true;
				SendClientMessage(playerid, -1, "{CCCCCC}Sua chamada foi enviada para os mecânicos disponíveis, aguarde.");
				SendClientMessage(playerid, -1, "{CCCCCC}Ligue para esse número novamente se você não precisar mais de um mecânico.");
				SendAlertToMechanic(playerid);
			}
			return 1;
		}
		case 6740: return SendClientMessage(playerid, -1, "{CCCCCC}Não ligue para o banco.");
		case 38350: return SendClientMessage(playerid, -1, "{CCCCCC}Asseguradora que faz o seguro dos seus veículos.");
		case 666:
		{
		    SetPlayerHealth(playerid, 1.0);
		    SetPlayerArmour(playerid, 1.0);
		    SetPlayerDrunkLevel(playerid, 5000);
		    SendClientMessage(playerid, -1, "Está procurando coisas erradas...");
		    PlayAudioStreamForPlayer(playerid, "https://www.dropbox.com/s/yiy6o8sj25k755n/666.mp3?dl=1");
		    SetTimerEx("meiameia", 240000, false, "i", playerid);
		    SetPlayerWeather(playerid, 100);
		}
	}
	
	new to_playerid = INVALID_PLAYER_ID;
	for(new i = 0, j = GetPlayerPoolSize(); i <= j; i++)
	{
		if(IsPlayerConnected(i))
		{
			if((PLAYER_TEMP[i][pt_GAME_STATE] == GAME_STATE_NORMAL) && PLAYER_PHONE[i][player_phone_NUMBER] == call_number)
			{
				to_playerid = i;
				break;
			}
		}
	}
	
	if(to_playerid == INVALID_PLAYER_ID) return SendClientMessage(playerid, -1, "{999999}O número que você está tentando ligar é incorreto ou esta fora de cobertura.");
	if(to_playerid == playerid) return SendClientMessage(playerid, -1, "{999999}O número que você está tentando ligar esta incorreto.");
	if(PLAYER_PHONE[to_playerid][player_phone_STATE] == PHONE_STATE_OFF) return SendClientMessage(playerid, -1, "{999999}O número que você está tentando ligar está desligado ou fora de cobertura.");
	if(PLAYER_TEMP[to_playerid][pt_PLAYER_IN_CALL]) return SendClientMessage(playerid, -1, "{999999}O número que você está tentando ligar está ocupado.");
	
	SendClientMessageEx(to_playerid, -1, "{e5f442}[Chamada recebida] {FFFFFF}%s está te ligando, para responder use {e5f442}/atender {FFFFFF}ou {e5f442}/encerrar.", convertPhoneNumber(to_playerid, PLAYER_PHONE[playerid][player_phone_NUMBER]));
	PLAYER_TEMP[to_playerid][pt_PLAYER_IN_CALL] = true;
	PLAYER_TEMP[to_playerid][pt_PLAYER_PHONE_CALL_STATE] = CALL_STATE_INCOMING_CALL;
	PLAYER_TEMP[to_playerid][pt_PLAYER_PHONE_CALL_PLAYERID] = playerid;
	
	SendClientMessageEx(playerid, -1, "Chamando {e5f442}%s ...", convertPhoneNumber(playerid, call_number));
	PLAYER_TEMP[playerid][pt_TIMERS][6] = SetTimerEx("NoCallResponse", 15000, false, "i", playerid);
	PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL] = true;
	PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_STATE] = CALL_STATE_WAITING_RESPONSE;
	PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_PLAYERID] = to_playerid;
	PlayerPlaySoundEx(playerid, 3600, 0.0, 0.0, 0.0);
	PlayerPlaySoundEx(to_playerid, 20600, 0.0, 0.0, 0.0);
	return 1;
}
CMD:atender(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_CRACK || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Não pode usar esse comando agora.");
	if(!PLAYER_PHONE[playerid][player_phone_VALID])
	{
		ShowPlayerMessage(playerid, "Você nao tem um celular, pode comprar um em qualquer 24/7.", 3, 1085);
		return 1;
	}
	if(PLAYER_PHONE[playerid][player_phone_STATE] == PHONE_STATE_OFF) return SendClientMessageEx(playerid, -1, "{999999}Seu telefone está desligado, para liga lo use /movil.");
	
	if(!PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL]) return SendClientMessageEx(playerid, -1, "{999999}Não há nenhuma chamada recebida.");
	if(PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_PLAYERID] == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, -1, "{999999}Não há nenhuma chamada recebida.");
	if(PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_STATE] != CALL_STATE_INCOMING_CALL) return SendClientMessageEx(playerid, -1, "{999999}Não há nenhuma chamada recebida.");
	
	new to_playerid = PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_PLAYERID];
	
	PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL] = true;
	PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_STATE] = CALL_STATE_ESTABLISHED;
	PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_PLAYERID] = to_playerid;
	SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
	
	KillTimer(PLAYER_TEMP[to_playerid][pt_TIMERS][6]);
	PLAYER_TEMP[to_playerid][pt_PLAYER_IN_CALL] = true;
	PLAYER_TEMP[to_playerid][pt_PLAYER_PHONE_CALL_STATE] = CALL_STATE_ESTABLISHED;
	PLAYER_TEMP[to_playerid][pt_PLAYER_PHONE_CALL_PLAYERID] = playerid;
	SetPlayerSpecialAction(to_playerid, SPECIAL_ACTION_USECELLPHONE);
	
	SendClientMessage(playerid, -1, "Chamada estabelecida, use {e5f442}/finalizar {FFFFFF}para termina-lá.");
	SendClientMessage(to_playerid, -1, "Chamada estabelecida, use {e5f442}/finalizar {FFFFFF}para termina-lá.");
	return 1;
}

CMD:finalizar(playerid, params[])
{
	if(!PLAYER_PHONE[playerid][player_phone_VALID])
	{
		ShowPlayerMessage(playerid, "Você nao tem um celular, pode ir a qualquer 24/7 para comprar um.", 3, 1085);
		return 1;
	}
	if(PLAYER_PHONE[playerid][player_phone_STATE] == PHONE_STATE_OFF) return SendClientMessageEx(playerid, -1, "{999999}Seu celular está desligado, para ligar use /celular");
	
	if(!PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL]) return SendClientMessageEx(playerid, -1, "{999999}Não há nenhuma chamada no momento.");
	
	if(PLAYER_TEMP[playerid][pt_POLICE_CALL_NAME] || PLAYER_TEMP[playerid][pt_POLICE_CALL_DESCRIPTION])
	{
		PLAYER_TEMP[playerid][pt_PLAYER_IN_CALL] = false;
		PLAYER_TEMP[playerid][pt_POLICE_CALL_NAME] = false;
		PLAYER_TEMP[playerid][pt_POLICE_CALL_DESCRIPTION] = false;
		SendClientMessage(playerid, -1, "{f47d42}Chamada finalizada.");
		return 1;
	}
	
	if(PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_PLAYERID] == INVALID_PLAYER_ID) return SendClientMessageEx(playerid, -1, "{999999}Não há nenhuma chamada no momento.");
	
	switch(PLAYER_TEMP[playerid][pt_PLAYER_PHONE_CALL_STATE])
	{
		case CALL_STATE_WAITING_RESPONSE:
		{
			EndPhoneCall(playerid);
			SendClientMessage(playerid, -1, "{CCCCCC}Finalizada.");
		}
		case CALL_STATE_INCOMING_CALL:
		{
			EndPhoneCall(playerid);
			SendClientMessage(playerid, -1, "{CCCCCC}Não atendeu a chamada.");
		}
		case CALL_STATE_ESTABLISHED:
		{
			EndPhoneCall(playerid);
			SendClientMessage(playerid, -1, "{f47d42}Chamada finalizada.");
		}
		default: SendClientMessageEx(playerid, -1, "{999999}Não está em nenhuma chamada.");
	}
	return 1;
}

