CMD:ahis(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_POLICE]) return SendClientMessage(playerid, -1, "{999999}Você não é policial.");
	if(PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_POLICE) return SendClientMessage(playerid, -1, "{999999}Não está em serviço como policial.");
	
	new to_player, reason[128];
	if(sscanf(params, "us[128]", to_player, reason)) return SendClientMessage(playerid, -1, "Erro: /ahis [ID ou Nome_Sobrenome] [Razão]");
	if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, -1, "Jogador desconectado.");
	if(to_player == playerid) return 1;
	
	if(PLAYER_TEMP[to_player][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "Não pode modificar o histórico desse jogador agora.");
	if(PLAYER_WORKS[to_player][WORK_POLICE]) return SendClientMessage(playerid, -1, "Não pode moficicar o histórico desse jogador por que é policial.");
	
	AddPlayerPoliceHistory(to_player, ACCOUNT_INFO[playerid][ac_NAME], reason);
	SendClientMessageEx(playerid, -1, "Foi adicionado uma informação ao histórico policial de %s.", PLAYER_TEMP[to_player][pt_RP_NAME]);
	return 1;
}

CMD:dhis(playerid, params[])
{
  if(!PLAYER_WORKS[playerid][WORK_POLICE]) return SendClientMessage(playerid, -1, "{999999}Você não é policial.");
  if(PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_POLICE) return SendClientMessage(playerid, -1, "{999999}Você não está em serviço como policial.");
  if(PLAYER_SKILLS[playerid][WORK_POLICE] < 11) return SendClientMessageEx(playerid, 0xFFA97FFF, "Deve ter no minimo o cargo %s para modificar o histórico policial.", POLICE_RANKS[11]);

  new to_player;
  if(sscanf(params, "u", to_player)) return SendClientMessage(playerid, -1, "Erro: /dhis [ID ou Nome_Sobrenome]");
  if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, -1, "Jogador desconectado.");
  if(PLAYER_TEMP[to_player][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "Não se poder apagar o histórico policial desse jogador agora.");

  new DB_Query[128];
  format(DB_Query, sizeof DB_Query, "DELETE FROM POLICE_HISTORY WHERE ID_USER = '%d';", ACCOUNT_INFO[to_player][ac_ID]);
  db_query(Database, DB_Query);

  SendClientMessageEx(playerid, -1, "Você deletou o histórico policial de %s.", PLAYER_TEMP[to_player][pt_RP_NAME]);
  return 1;
}

