CMD:capo(playerid, params[])//todas
{
    new vehicleid = INVALID_VEHICLE_ID;
   	new options[24];
	if(sscanf(params, "s[24]", options)) return SendClientMessage(playerid, -1, "{999999}Erro, uso correto do comando {CCCCCC}/capo [abrir - fechar].");

	if(!strcmp(options, "abrir", true))
	{
	if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, 0xFFFFFFAA, "Você não está em um veículo.");
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) vehicleid = GetPlayerVehicleID(playerid);
	else if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) vehicleid = GetPlayerCameraTargetVehicle(playerid);

	if(vehicleid == INVALID_VEHICLE_ID) return SendClientMessage(playerid, -1, "{999999}Não está em seu veículo ou perto dele para abri-lo.");
	if(!PLAYER_VEHICLES[vehicleid][player_vehicle_VALID]) return SendClientMessage(playerid, -1, "{999999}Este não é seu veículo.");
	if(PLAYER_VEHICLES[vehicleid][player_vehicle_OWNER_ID] != ACCOUNT_INFO[playerid][ac_ID]) return SendClientMessage(playerid, -1, "{999999}Este não é seu veículo.");

	GLOBAL_VEHICLES[vehicleid][gb_vehicle_PARAMS_BONNET] = 1;
	UpdateVehicleParams(vehicleid);
	Auto_SendPlayerAction(playerid, "abriu capo de seu veículo.");
	}
	if(!strcmp(options, "fechar", true))
	{
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER) return SendClientMessage(playerid, 0xFFFFFFAA, "Você deve estar apé para fechar o capo.");
	if(GetPlayerState(playerid) == PLAYER_STATE_DRIVER || GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) vehicleid = GetPlayerVehicleID(playerid);
	else if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT) vehicleid = GetPlayerCameraTargetVehicle(playerid);

	if(vehicleid == INVALID_VEHICLE_ID) return SendClientMessage(playerid, -1, "{999999}Não está em seu veículo ou perto dele para abri-lo.");
	if(!PLAYER_VEHICLES[vehicleid][player_vehicle_VALID]) return SendClientMessage(playerid, -1, "{999999}Este não é seu veículo.");
	if(PLAYER_VEHICLES[vehicleid][player_vehicle_OWNER_ID] != ACCOUNT_INFO[playerid][ac_ID]) return SendClientMessage(playerid, -1, "{999999}Este não é seu veículo.");

	GLOBAL_VEHICLES[vehicleid][gb_vehicle_PARAMS_BONNET] = 0;
	UpdateVehicleParams(vehicleid);
	Auto_SendPlayerAction(playerid, "fechou capo de seu veículo.");
	}
	return 1;
}

