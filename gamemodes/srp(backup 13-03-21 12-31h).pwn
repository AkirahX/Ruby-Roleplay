	SendClientMessageEx(params[0], -1, "{CCCCCC}%s %c. %s {A9C4E4}[Distintivo: %d]", POLICE_RANKS[ PLAYER_SKILLS[playerid][WORK_POLICE] ], PLAYER_TEMP[playerid][pt_FIRST_NAME][0], PLAYER_TEMP[playerid][pt_SUB_NAME], PLAYER_MISC[playerid][MISC_PLACA_PD]);
	return 1;
}

