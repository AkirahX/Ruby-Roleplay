CMD:convidar(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_CRACK || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Não pode usar esse comando agora.");
	
	if(!PLAYER_CREW[playerid][player_crew_VALID]) return SendClientMessage(playerid, -1, "{999999}Você não é membro de nenhuma facção.");
	if(!CREW_RANK_INFO[ PLAYER_CREW[playerid][player_crew_INDEX] ][ PLAYER_CREW[playerid][player_crew_RANK] ][crew_rank_PERMISSION][CREW_RANK_INVITE]) return SendClientMessage(playerid, -1, "{999999}No tienes permiso para invitar personas a la banda.");
	if(CREW_INFO[ PLAYER_CREW[playerid][player_crew_INDEX] ][crew_FIGHTING]) return SendClientMessage(playerid, -1, "{999999}No puedes invitar a gente a la banda cuando la banda está en combate.");
	
	new to_player;
	if(sscanf(params, "u", to_player)) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/convidar {FFFFFF}[ID ou Nome_Sobrenome]");
	if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, -1, "{999999}O jogador está desconectado.");
	if(to_player == playerid) return SendClientMessage(playerid, -1, "{999999}É você.");
	
	new Float:pos[3]; GetPlayerPos(to_player, pos[0], pos[1], pos[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, pos[0], pos[1], pos[2])) return SendClientMessage(playerid, 0x999999AA, "Este jogador não está perto de você.");
	if(PLAYER_WORKS[to_player][WORK_POLICE]) return SendClientMessage(playerid, -1, "{999999}Essa pessoa é um policial e não pode ter uma facção.");
	if(PLAYER_CREW[to_player][player_crew_VALID]) return SendClientMessage(playerid, -1, "{999999}Essa pessoa pertence a outra facção.");
	if(PLAYER_TEMP[to_player][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "{999999}Você não pode convidar esse jogador agora.");
	
	if(gettime() < PLAYER_TEMP[ to_player ][pt_LAST_GOT_CREW] + 600)
	{
		new time = 600 - (gettime() - PLAYER_TEMP[to_player][pt_LAST_GOT_CREW]);
		SendClientMessageEx(playerid, -1, "{999999}A outra pessoa deve esperar %s minutos para ser convidada novamente.", TimeConvert(time));
		return 1;
	}
	
	PLAYER_TEMP[playerid][pt_CREW_INVITE_PID] = to_player;
	PLAYER_TEMP[playerid][pt_CREW_INVITE_AID] = ACCOUNT_INFO[to_player][ac_ID];
	SendClientMessageEx(playerid, -1, "Se você quiser convidar %s para a facção, escolha o cargo que dará a eles.", PLAYER_TEMP[to_player][pt_RP_NAME]);
	ShowDialog(playerid, DIALOG_CREW_INVITE_RANK);
	return 1;
}

