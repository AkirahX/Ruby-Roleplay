CMD:lixeiro(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_TRASH]) return SendClientMessage(playerid, -1, "{999999}Você não é lixeiro.");
	if(PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_NONE && PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_TRASH)
	{
		SendClientMessageEx(playerid, -1, "{999999}Deve deixar de estar em serviço como %s primero.", work_info[ PLAYER_TEMP[playerid][pt_WORKING_IN] ][work_info_NAME]);
		return 1;
	}
	
	if(GetPlayerVirtualWorld(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerInterior(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, -1, "{999999}Não está a pé.");
	if(!IsPlayerInRangeOfPoint(playerid, 1.0, -1906.577514, -1756.457519, 22.079319)) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	
	if(!PLAYER_TEMP[playerid][pt_WORKING_IN]) StartPlayerJob(playerid, WORK_TRASH);
	else EndPlayerJob(playerid);
	return 1;
}
