CMD:rebocar(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_MECHANIC]) return SendClientMessage(playerid, -1, "{999999}Você não é mecânico.");
	if(PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_MECHANIC) return SendClientMessage(playerid, -1, "{999999}Você não está em serviço como mecânico.");
	if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, -1, "{999999}Você não está conduzindo um guincho.");
	
	new vehicleid = GetPlayerVehicleID(playerid);
	if(GLOBAL_VEHICLES[vehicleid][gb_vehicle_TYPE] != VEHICLE_TYPE_WORK) return SendClientMessage(playerid, -1, "{999999}Você não está conduzindo um guincho.");
	if(WORK_VEHICLES[vehicleid][work_vehicle_WORK] != WORK_MECHANIC) return SendClientMessage(playerid, -1, "{999999}Você não está conduzindo um guincho.");
	
	new to_player;
	if(sscanf(params, "d", to_player)) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/rebocar {FFFFFF}[ID ou Nome]");
	if(!IsPlayerConnected(to_player)) return SendClientMessage(playerid, -1, "{999999}Jogador desconectado.");
	
	new Float:pos[3]; GetPlayerPos(to_player, pos[0], pos[1], pos[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 10.0, pos[0], pos[1], pos[2])) return SendClientMessage(playerid, 0x999999AA, "Este jogador não está próximo a você.");
	if(PLAYER_TEMP[to_player][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "{999999}Não pode rebocar este jogador agora.");
	if(!PLAYER_TEMP[to_player][pt_WANT_MECHANIC]) return SendClientMessage(playerid, -1, "{999999}Este jogador não solicitou um mecânico.");
	
	
	
	new player_vehicleid = GetPlayerVehicleID(to_player);
	if(!player_vehicleid)
	{
		SendClientMessage(to_player, -1, "{999999}Para que o mecânico possa rebocar seu veículo vá no assento de passageiro.");
		SendClientMessage(playerid, -1, "{999999}Esta pessoa não tem nenhum veículo para rebocar.");
		return 1;
	}

	
	if(!VEHICLE_INFO[GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_MODELID] - 400][vehicle_info_MECHANIC_TRAILER]) return SendClientMessage(playerid, -1, "{999999}Não se pode rebocar este tipo de veículo, para repara-lo desça do guincho e use /vmenu próximo ao veículo.");
	if(VEHICLE_INFO[GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_MODELID] - 400][vehicle_info_SEATS] < 2) return SendClientMessage(playerid, -1, "{999999}Não se pode rebocar este tipo de veículo, para repara-lo desça do guincho e use /vmenu próximo ao veículo.");
	
	if(GetPlayerState(to_player) != PLAYER_STATE_PASSENGER)
	{
		SendClientMessage(playerid, -1, "Para que o mecânico possa rebocar seu veículo vá no assento de passageiro.");
		SendClientMessage(playerid, -1, "{999999}A pessoa deve estar no assento de passageiro de seu veículo.");
		return 1;
	}
	
	if(GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_OCCUPIED])
	{
		SendClientMessage(playerid, -1, "Para que o mecânico possa rebocar seu veículo vá no assento de passageiro.");
		SendClientMessage(playerid, -1, "{999999}Para rebocar o veículo o mesmo deve estar vazio.");
		return 1;
	}
	
	if(GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_ATTACHED_TO] != INVALID_VEHICLE_ID)
	{
		new trailerid = GetVehicleTrailer(GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_ATTACHED_TO]);
		if(trailerid == player_vehicleid) return SendClientMessage(playerid, -1, "{999999}Este veículo já está rebocado.");
	}
		
	if(GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_PARAMS_ENGINE])
	{
		GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_PARAMS_ENGINE] = 0;
		UpdateVehicleParams(player_vehicleid);
	}
	
	GLOBAL_VEHICLES[player_vehicleid][gb_vehicle_ATTACHED_TO] = vehicleid;
	AttachTrailerToVehicle(player_vehicleid, vehicleid);
	
	SendClientMessage(playerid, -1, "{CCCCCC}Veículo rebocado, leve para a garagem para concerta-lo.");
	SendClientMessage(to_player, -1, "{CCCCCC}O mecânico rebocou o veículo.");
	
	PLAYER_TEMP[to_player][pt_WANT_MECHANIC] = false;
	DisablePlayerMechanicMark(to_player);
	return 1;
}

CMD:vmenu(playerid, params[])
{
