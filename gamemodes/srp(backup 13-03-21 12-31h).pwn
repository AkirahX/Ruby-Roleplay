	{
		if(extra < 0 || extra > CHARACTER_INFO[playerid][ch_CASH]) return SendClientMessage(playerid, -1, "{999999}Quantidade de dinheiro incorreta.");
		
		GivePlayerCash(playerid, -extra);
		
		SendClientMessageEx(playerid, -1, "{CCCCCC}Você soltou {5cd1d1}%s$.", number_format_thousand(extra));
		Auto_SendPlayerAction(playerid, "solta dinheiro no chão.");
		return 1;
	}
	else if(!strcmp(option, "alimento", true))
	{
		if(extra < 1 || extra > MAX_PLAYER_POCKET_OBJECTS) return SendClientMessageEx(playerid, -1, "Erro: {C4FF66}/soltar alimento {FFFFFF}[SLOT 1-%d]", MAX_PLAYER_POCKET_OBJECTS);
	
		extra --;
		
		if(!PLAYER_POCKET[playerid][extra][player_pocket_VALID]) return SendClientMessage(playerid, -1, "{999999}Esse slot está vazio.");

		SendClientMessageEx(playerid, -1, "{999999}Você jogou fora um/a \"%s\".", PLAYER_POCKET[playerid][extra][player_pocket_object_NAME]);
		DeletePlayerPocketObject(playerid, extra);
		Auto_SendPlayerAction(playerid, "solta alimento no chão.");
		return 1;
	}
	else if(!strcmp(option, "medicamentos", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_MEDICINE]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_MEDICINE] -= extra;
		
		SendClientMessageEx(playerid, -1, "Você soltou %dg de medicamentos.", extra);
		Auto_SendPlayerAction(playerid, "solta medicamentos no chão.");
		return 1;
	}
	else if(!strcmp(option, "maconha", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_CANNABIS]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_CANNABIS] -= extra;
		
		SendClientMessageEx(playerid, -1, "Você soltou %dg de maconha.", extra);
		Auto_SendPlayerAction(playerid, "solta maconha no chão.");
		return 1;
	}
	else if(!strcmp(option, "cocaina", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_CRACK]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_CRACK] -= extra;
		
		SendClientMessageEx(playerid, -1, "Você soltou %dg de cocaína.", extra);
		Auto_SendPlayerAction(playerid, "solta crack no chão.");
		return 1;
	}
	else if(!strcmp(option, "arma", false))
	{
		if(extra < 0 || extra > 12) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/soltar arma {FFFFFF}[Slot /armas]");

		if(!PLAYER_WEAPONS[playerid][extra][player_weapon_VALID]) return SendClientMessage(playerid, -1, "{999999}Não há nenhuma arma nesse slot.");

		SendClientMessageEx(playerid, -1, "{999999}Você soltou sua '%s'.", WEAPON_INFO[ PLAYER_WEAPONS[playerid][extra][player_weapon_ID] ][weapon_info_NAME]);
		RemovePlayerSlotWeapon(playerid, extra, true);
		Auto_SendPlayerAction(playerid, "solta sua arma no chão.");
	}
	else SendClientMessage(playerid, -1, "{999999}Erro nos parâmetros, use {CCCCCC}/manual soltar.");
	return 1;
}
CMD:entrar(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_CRACK || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Você não pode usar este comando agora.");
	if(PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID] == 0) return ShowPlayerMessage(playerid, " ", 3);
		
	new info[3];
	Streamer_GetArrayData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_EXTRA_ID, info);
	if(info[0] == PICKUP_TYPE_NONE) return ShowPlayerMessage(playerid, " ", 3);
	
	new Float:pos[3]; 
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_X, pos[0]);
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_Y, pos[1]);
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_Z, pos[2]);
	
	if(!IsPlayerInRangeOfPoint(playerid, 1.0, pos[0], pos[1], pos[2])) return ShowPlayerMessage(playerid, " ", 3);
	
	switch(info[0])
	{
		case PICKUP_TYPE_ENTER_EXIT:
		{
			PLAYER_TEMP[playerid][pt_HOSPITAL] = GetNearestHospitalForPlayer(playerid);
			if(PLAYER_TEMP[playerid][pt_HOSPITAL] == -1) PLAYER_TEMP[playerid][pt_HOSPITAL] = 1;
	
			if(info[2] == 2) // Está en el Pickup Exterior y quiere ir al interior
			{
				if(ENTER_EXIT[info[1]][ee_TIME_OPEN] == 0 && ENTER_EXIT[info[1]][ee_TIME_CLOSE] == 0) // 24 horas
				{
					CHARACTER_INFO[playerid][ch_STATE] = ROLEPLAY_STATE_INTERIOR;
					CHARACTER_INFO[playerid][ch_INTERIOR_EXTRA] = ENTER_EXIT[info[1]][ee_ID];
					PLAYER_TEMP[playerid][pt_INTERIOR_INDEX] = info[1];
					SetPlayerPosEx(playerid, ENTER_EXIT[info[1]][ee_INT_X], ENTER_EXIT[info[1]][ee_INT_Y], ENTER_EXIT[info[1]][ee_INT_Z], ENTER_EXIT[info[1]][ee_INT_ANGLE], ENTER_EXIT[info[1]][ee_INT_INTERIOR], ENTER_EXIT[info[1]][ee_INT_WORLD], false /*ENTER_EXIT[info[1]][ee_INT_FREEZE]*/, true);
					FreezePlayer(playerid);
				}
				else
				{
					if(is_open(SERVER_TIME[0], ENTER_EXIT[info[1]][ee_TIME_OPEN], ENTER_EXIT[info[1]][ee_TIME_CLOSE]))
					{
						CHARACTER_INFO[playerid][ch_STATE] = ROLEPLAY_STATE_INTERIOR;
						CHARACTER_INFO[playerid][ch_INTERIOR_EXTRA] = ENTER_EXIT[info[1]][ee_ID];
						PLAYER_TEMP[playerid][pt_INTERIOR_INDEX] = info[1];
						SetPlayerPosEx(playerid, ENTER_EXIT[info[1]][ee_INT_X], ENTER_EXIT[info[1]][ee_INT_Y], ENTER_EXIT[info[1]][ee_INT_Z], ENTER_EXIT[info[1]][ee_INT_ANGLE], ENTER_EXIT[info[1]][ee_INT_INTERIOR], ENTER_EXIT[info[1]][ee_INT_WORLD], false /*ENTER_EXIT[info[1]][ee_INT_FREEZE]*/, true);
						FreezePlayer(playerid);
						
						if(ENTER_EXIT[info[1]][ee_INTERIOR_TYPE] == INTERIOR_CLUB) PlayAudioStreamForPlayerEx(playerid, "https://www.dropbox.com/s/yeuwio26i7y00ly/striptease.mp3?dl=1");
					}
					else SendClientMessageEx(playerid, -1, "{C4FF66}%s {FFFFFF}está fechado, horário de funcionamento: {C4FF66}%02d:00 - %02d:00.", ENTER_EXIT[info[1]][ee_NAME], ENTER_EXIT[info[1]][ee_TIME_OPEN], ENTER_EXIT[info[1]][ee_TIME_CLOSE]);
				}
			}
			else ShowPlayerMessage(playerid, " ", 3);
		}
		case PICKUP_TYPE_PROPERTY:
		{
			PLAYER_TEMP[playerid][pt_HOSPITAL] = GetNearestHospitalForPlayer(playerid);
			if(PLAYER_TEMP[playerid][pt_HOSPITAL] == -1) PLAYER_TEMP[playerid][pt_HOSPITAL] = 1;
			
			if(info[2] == 2) // Está en el Pickup Exterior y quiere ir al interior
			{
				if(!PROPERTY_INFO[info[1]][property_SOLD]) return SendClientMessage(playerid, -1, "{999999}Essa propriedade está a venda.");
				if(PROPERTY_INFO[info[1]][property_CREW])
				{
					if(!PLAYER_CREW[playerid][player_crew_VALID]) return SendClientMessageEx(playerid, -1, "{999999}Somente membros da facção podem entrar.");
					if(PLAYER_CREW[playerid][player_crew_ID] != PROPERTY_INFO[info[1]][property_CREW_ID]) return SendClientMessageEx(playerid, -1, "{999999}Somente membros da facção podem entrar.");
					
					CHARACTER_INFO[playerid][ch_STATE] = ROLEPLAY_STATE_GUEST_PROPERTY;
					CHARACTER_INFO[playerid][ch_INTERIOR_EXTRA] = PROPERTY_INFO[info[1]][property_ID];
					PLAYER_TEMP[playerid][pt_PROPERTY_INDEX] = info[1];
					SetPlayerPosEx(playerid, PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_X], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Y], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Z], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_ANGLE], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_INTERIOR], PROPERTY_INFO[info[1]][property_ID], false /*PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_FREEZE]*/, true);
					FreezePlayer(playerid);
				}
				else
				{
					if(PROPERTY_INFO[info[1]][property_OWNER_ID] == ACCOUNT_INFO[playerid][ac_ID])
					{
						CHARACTER_INFO[playerid][ch_STATE] = ROLEPLAY_STATE_OWN_PROPERTY;
						CHARACTER_INFO[playerid][ch_INTERIOR_EXTRA] = PROPERTY_INFO[info[1]][property_ID];
						PLAYER_TEMP[playerid][pt_PROPERTY_INDEX] = info[1];
						SetPlayerPosEx(playerid, PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_X], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Y], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Z], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_ANGLE], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_INTERIOR], PROPERTY_INFO[info[1]][property_ID], false /*PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_FREEZE]*/, true);
						FreezePlayer(playerid);
					}
					else
					{
						if(gettime() < PLAYER_TEMP[playerid][pt_ANTIFLOOD_KNOCK_PROPERTY] + 10) return SendClientMessage(playerid, 0x999999AA, "Espere para tocar novamente a campainha.");
						
						new owner_playerid = GetOwnerIntProperty(PROPERTY_INFO[info[1]][property_ID]);
						if(owner_playerid == -1) return SendClientMessage(playerid, -1, "{999999}Essa não é sua propriedade e o dono não está em casa para te deixar entrar.");
						
						PLAYER_TEMP[playerid][pt_ANTIFLOOD_KNOCK_PROPERTY] = gettime();
						PLAYER_TEMP[owner_playerid][pt_KNOCK_PLAYER_ID] = playerid;
						SendClientMessageEx(owner_playerid, -1, "{f4e542}%s {FFFFFF}tocou a campainha, para deixar entrar vá até a porta e use {bef441}/porta.", PLAYER_TEMP[playerid][pt_RP_NAME]);
						SendClientMessage(playerid, -1, "{CCCCCC}Você tocou a campainha, espere para que abram ou vá embora.");
					}
				}
			}
			else ShowPlayerMessage(playerid, " ", 3);
		}
		default: ShowPlayerMessage(playerid, " ", 3);
	}
	return 1;
}

CMD:porta(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] != ROLEPLAY_STATE_OWN_PROPERTY) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID] == 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
		
	new info[3];
	Streamer_GetArrayData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_EXTRA_ID, info);
	if(info[0] != PICKUP_TYPE_PROPERTY) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	
	new Float:pos[3]; 
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_X, pos[0]);
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_Y, pos[1]);
	Streamer_GetFloatData(STREAMER_TYPE_PICKUP, PLAYER_TEMP[playerid][pt_LAST_PICKUP_ID], E_STREAMER_Z, pos[2]);
	
	if(!IsPlayerInRangeOfPoint(playerid, 1.0, pos[0], pos[1], pos[2])) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	
	if(info[2] == 1) // Está en el Pickup Interior
	{
		if(PROPERTY_INFO[info[1]][property_OWNER_ID] == ACCOUNT_INFO[playerid][ac_ID])
		{
			if(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID] == INVALID_PLAYER_ID) return SendClientMessage(playerid, 0x999999AA, "Ninguém tocou a campainha.");
			if(!IsPlayerConnected(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID])) return SendClientMessage(playerid, 0x999999AA, "Ninguém tocou a campainha ou já se foi.");
			if(!IsPlayerInRangeOfPoint(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID], 3.0, PROPERTY_INFO[info[1]][property_EXT_X], PROPERTY_INFO[info[1]][property_EXT_Y], PROPERTY_INFO[info[1]][property_EXT_Z])) return SendClientMessage(playerid, 0x999999AA, "Ninguém tocou a campainha ou já se foi.");
			if(PLAYER_TEMP[playerid][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, 0x999999AA, "Está pessoa não pode entrar agora.");
			
			PLAYER_TEMP[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][pt_HOSPITAL] = GetNearestHospitalForPlayer(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]);
			if(PLAYER_TEMP[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][pt_HOSPITAL] == -1) PLAYER_TEMP[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][pt_HOSPITAL] = 1;
			
			CHARACTER_INFO[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][ch_STATE] = ROLEPLAY_STATE_GUEST_PROPERTY;
			CHARACTER_INFO[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][ch_INTERIOR_EXTRA] = PROPERTY_INFO[info[1]][property_ID];
			PLAYER_TEMP[PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]][pt_PROPERTY_INDEX] = info[1];
			SetPlayerPosEx(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_X], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Y], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_Z], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_ANGLE], PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_INTERIOR], PROPERTY_INFO[info[1]][property_ID], false /*PROPERTY_INTERIORS[ PROPERTY_INFO[info[1]][property_ID_INTERIOR] ][property_INT_FREEZE]*/, true);
			FreezePlayer(PLAYER_TEMP[playerid][pt_KNOCK_PLAYER_ID]);
		}
		else ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	}
	else ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	return 1;
}

