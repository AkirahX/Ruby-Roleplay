CMD:plantar(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_FARMER]) return SendClientMessage(playerid, -1, "{999999}Você não é agricultor.");
	if(PLAYER_TEMP[playerid][pt_WORKING_IN] && PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_FARMER)
	{
		SendClientMessageEx(playerid, -1, "{999999}Deve deixar de estar em serviço como %s primero.", work_info[ PLAYER_TEMP[playerid][pt_WORKING_IN] ][work_info_NAME]);
		return 1;
	}
	
	if(GetPlayerVirtualWorld(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerInterior(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, -1, "{999999}Deve estar a pé.");
	if(!IsPlayerInDynamicArea(playerid, Farmer_Area)) return SendClientMessage(playerid, 0x999999AA, "Você não está na zona de plantação, elá está marcada no mapa com uma bandeira.");
	if(PLAYER_TEMP[playerid][pt_PLANTING]) return SendClientMessage(playerid, 0x999999AA, "Você já está plantando.");
	
	if(gettime() < PLAYER_TEMP[playerid][pt_LAST_PLANT_TIME] + 60)
	{
		new time = (60-(gettime()-PLAYER_TEMP[playerid][pt_LAST_PLANT_TIME]));
		SendClientMessageEx(playerid, 0x999999AA, "Deve esperar %s minutos para voltar a plantar.", TimeConvert(time));
		return 1;
	}
	
	if(GetPlayerPlantedPlants(playerid) > 10) return SendClientMessage(playerid, 0x999999AA, "Por favor, recolha suas outras plantas para poder plantar mais.");
	
	for(new i = 0; i != MAX_PLANTS; i ++)
	{
		if(!PLANTS[i][plant_VALID]) continue;
		
		new Float:pos[3];
		GetDynamicObjectPos(PLANTS[i][plant_OBJECT_ID], pos[0], pos[1], pos[2]);
		if(IsPlayerInRangeOfPoint(playerid, 3.0, pos[0], pos[1], pos[2]))
		{
			SendClientMessage(playerid, -1, "{999999}Já há uma planta por aqui, se afaste um pouco.");
			return 1;
		}
	}
	
	ShowDialog(playerid, DIALOG_PLANT_PLANTS);
	return 1;
}

CMD:pecas(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_MECHANIC]) return SendClientMessage(playerid, -1, "{999999}Você não é mecânico.");
	if(GetPlayerVirtualWorld(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerInterior(playerid) != 0) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, -1, "{999999}Deve estar a pé.");
	if(!IsPlayerInRangeOfPoint(playerid, 1.0, -80.710136, -1193.329956, 2.947619)) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	
	if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/pecas {FFFFFF}[quantidade]");
	if(params[0] <= 0 || params[0] >= 10000) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/pecas {FFFFFF}[quantidade > 0]");
	
	new price = params[0] * 50;
	if(price > CHARACTER_INFO[playerid][ch_CASH])
	{
		SendClientMessageEx(playerid, -1, "{999999}Precisa de %s$ para poder comprar %d peças.", number_format_thousand(price), params[0]);
		return 1;
	}
	
	GivePlayerCash(playerid, -price);
	PLAYER_MISC[playerid][MISC_MECHANIC_PIECES] += params[0];
	
	SendClientMessageEx(playerid, -1, "{CCCCCC}Você comprou %s peças por %s$, agora tem %s peças.", number_format_thousand(params[0]), number_format_thousand(price), number_format_thousand(PLAYER_MISC[playerid][MISC_MECHANIC_PIECES]));
	return 1;
}

