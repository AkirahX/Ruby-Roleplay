#define TIME_BETWEEN_GIVE_CASH	30 // segundos
CMD:dar(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Não pode usar este comando no momento.");
	if(ACCOUNT_INFO[playerid][ac_LEVEL] < 2) return SendClientMessage(playerid, -1, "{999999}Deve ser no minimo nível 2 para usar este comando.");
	
	new option[24], to_playerid, extra;
	if(sscanf(params, "s[24]ud", option, to_playerid, extra)) return SendClientMessage(playerid, -1, "{999999}Erro nos parâmetros, use {CCCCCC}/manual dar.");
	
	if(!IsPlayerConnected(to_playerid)) return SendClientMessage(playerid, -1, "{999999}Jogador desconectado.");
	new Float:pos[3]; GetPlayerPos(to_playerid, pos[0], pos[1], pos[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, pos[0], pos[1], pos[2])) return SendClientMessage(playerid, 0x999999AA, "Este jogador não está próximo a você.");
	if(PLAYER_TEMP[to_playerid][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "{999999}Você não pode dar nada a esse jogador agora.");
	
	if(!strcmp(option, "dinheiro", true))
	{
		if(gettime() < PLAYER_TEMP[playerid][pt_LAST_GIVE_MONEY_TIME] + TIME_BETWEEN_GIVE_CASH)
		{
			new time = TIME_BETWEEN_GIVE_CASH - (gettime() - PLAYER_TEMP[playerid][pt_LAST_GIVE_MONEY_TIME]);
			SendClientMessageEx(playerid, -1, "{999999}Deve esperar %s minutos para dar dinheiro novamente.", TimeConvert(time));
			return 1;
		}
		
		if(extra < 0 || extra > CHARACTER_INFO[playerid][ch_CASH]) return SendClientMessage(playerid, -1, "{999999}Quantia incorreta.");
		if(extra > 50000) return SendClientMessage(playerid, -1, "{999999}Para dar tanto dinheiro realize uma transferência bancária.");
		
		GivePlayerCash(playerid, -extra);
		GivePlayerCash(to_playerid, extra);
		
		SendClientMessageEx(playerid, -1, "Você deu {5cd1d1}%s$ {FFFFFF}para {b6f442}%s.", number_format_thousand(extra), PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		SendClientMessageEx(to_playerid, -1, "{b6f442}%s {FFFFFF} te deu {5cd1d1}%s$.", PLAYER_TEMP[playerid][pt_RP_NAME], number_format_thousand(extra));
		
		new action[64];
		format(action, sizeof action, "da dinheiro para %s.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		PLAYER_TEMP[playerid][pt_LAST_GIVE_MONEY_TIME] = gettime();
		return 1;
	}
	else if(!strcmp(option, "alimento", true))
	{
		if(extra < 1 || extra > MAX_PLAYER_POCKET_OBJECTS) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/dar alimento {FFFFFF}[ID ou Nome_Sobrenome] [Slot em /alimentos]");
	
		extra --;
		
		if(!PLAYER_POCKET[playerid][extra][player_pocket_VALID]) return SendClientMessage(playerid, -1, "{999999}Você não tem nada nesse slot.");
		
		new to_player_slot = GetEmptyPlayerPocketSlot(to_playerid);
		if(to_player_slot == -1)
		{
			PlayerPlaySoundEx(playerid, 1085, 0.0, 0.0, 0.0);
			SendClientMessage(playerid, -1, "{999999}O jogador não tem mais espaço entre seus alimentos.");
			return 1;
		}
		
		new action[64];
		format(action, sizeof action, "da sua %s para %s.", PLAYER_POCKET[playerid][extra][player_pocket_object_NAME], PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		
		TransferPlayerPocketObject(playerid, extra, to_playerid, to_player_slot);
		return 1;
	}
	else if(!strcmp(option, "medicamentos", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_MEDICINE]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_MEDICINE] -= extra;
		PLAYER_MISC[to_playerid][MISC_MEDICINE] += extra;
		
		SendClientMessageEx(playerid, -1, "Você deu %dg de medicamentos para %s.", extra, PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		SendClientMessageEx(to_playerid, -1, "%s te deu %dg de medicamentos.", PLAYER_TEMP[playerid][pt_RP_NAME], extra);
		
		new action[64];
		format(action, sizeof action, "da algo para %s.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		return 1;
	}
	else if(!strcmp(option, "maconha", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_CANNABIS]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_CANNABIS] -= extra;
		PLAYER_MISC[to_playerid][MISC_CANNABIS] += extra;
		
		SendClientMessageEx(playerid, -1, "Você deu %dg de maconha para %s.", extra, PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		SendClientMessageEx(to_playerid, -1, "%s te deu %dg de maconha.", PLAYER_TEMP[playerid][pt_RP_NAME], extra);
		
		new action[64];
		format(action, sizeof action, "da algo para %s.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		return 1;
	}
	else if(!strcmp(option, "cocaina", true))
	{
		if(extra <= 0 || extra > 10000000) return SendClientMessage(playerid, -1, "{999999}Quantidade incorreta.");
		if(extra > PLAYER_MISC[playerid][MISC_CRACK]) return SendClientMessage(playerid, -1, "{999999}Você não tem essa quantidade.");
		
		PLAYER_MISC[playerid][MISC_CRACK] -= extra;
		PLAYER_MISC[to_playerid][MISC_CRACK] += extra;
		
		SendClientMessageEx(playerid, -1, "Você deu %dg de cocaína para %s.", extra, PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		SendClientMessageEx(to_playerid, -1, "%s te deu %dg de cocaína.", PLAYER_TEMP[playerid][pt_RP_NAME], extra);
		
		new action[64];
		format(action, sizeof action, "da algo para %s.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		return 1;
	}
	else if(!strcmp(option, "arma", true))
	{
		if(PLAYER_WORKS[playerid][WORK_POLICE]) return SendClientMessage(playerid, -1, "{999999}Os policiais não podem vender armas.");
		if(extra < 0 || extra > 12) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/dar arma {FFFFFF}[ID ou Nome_Sobrenome] [Slot em /armas]");
	
		if(!PLAYER_WEAPONS[playerid][extra][player_weapon_VALID]) return SendClientMessage(playerid, -1, "{999999}Você não tem nenhuma arma neste slot.");
		
		if(ACCOUNT_INFO[to_playerid][ac_LEVEL] < 2) return SendClientMessage(playerid, -1, "{999999}A outra pessoa deve ser no minimo nível 2.");
		if(PLAYER_WEAPONS[to_playerid][extra][player_weapon_VALID])
		{
			PlayerPlaySoundEx(playerid, 1085, 0.0, 0.0, 0.0);
			SendClientMessage(playerid, -1, "{999999}Essa pessoa já tem uma arma no mesmo slot.");
			return 1;
		}
		
		SendClientMessageEx(playerid, -1, "{CCCCCC}Você deu sua '%s' para %s.", WEAPON_INFO[ PLAYER_WEAPONS[playerid][extra][player_weapon_ID] ][weapon_info_NAME], PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		SendClientMessageEx(to_playerid, -1, "{CCCCCC}%s te deu sua '%s'.", PLAYER_TEMP[playerid][pt_RP_NAME], WEAPON_INFO[ PLAYER_WEAPONS[playerid][extra][player_weapon_ID] ][weapon_info_NAME]);
		
		new action[64];
		format(action, sizeof action, "entrega uma arma para %s.", PLAYER_TEMP[to_playerid][pt_RP_NAME]);
		Auto_SendPlayerAction(playerid, action);
		
		TransferPlayerWeapon(playerid, extra, to_playerid);
		return 1;
	}
	else SendClientMessage(playerid, -1, "{999999}Erro nos parâmetros, use {CCCCCC}/manual dar.");
	return 1;
}

CMD:vender(playerid, params[])
{
	if(CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_JAIL || CHARACTER_INFO[playerid][ch_STATE] == ROLEPLAY_STATE_ARRESTED) return SendClientMessage(playerid, -1, "{999999}Agora você não pode usar esse comando.");
	if(ACCOUNT_INFO[playerid][ac_LEVEL] < 2) return SendClientMessage(playerid, -1, "{999999}Deve ser no minimo nível 2 para usar este comando.");
	
	new option[24], to_playerid, extra, price;
	if(sscanf(params, "s[24]udd", option, to_playerid, extra, price)) return SendClientMessage(playerid, -1, "{999999}Erros nos parametros, ultilize {CCCCCC}/manual vender.");
	
	if(price <= 0 || price > 10000000) return SendClientMessage(playerid, -1, "{999999}Preço invalido.");
	if(!IsPlayerConnected(to_playerid)) return SendClientMessage(playerid, -1, "{999999}O jogador não esta conectado.");
	new Float:pos[3]; GetPlayerPos(to_playerid, pos[0], pos[1], pos[2]);
	if(!IsPlayerInRangeOfPoint(playerid, 2.0, pos[0], pos[1], pos[2])) return SendClientMessage(playerid, 0x999999AA, "Esse jogador não está próximo a você.");
	if(PLAYER_TEMP[to_playerid][pt_GAME_STATE] != GAME_STATE_NORMAL) return SendClientMessage(playerid, -1, "{999999}Não pode vender nada para esse jogador agora.");
	
	if(price > CHARACTER_INFO[to_playerid][ch_CASH])
	{
		SendClientMessage(playerid, -1, "{999999}O jogador não tem essa quantidade de dinheiro.");
		return 1;
	}
	
	PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_PID] = playerid;
	PLAYER_TEMP[to_playerid][pt_TRICK_SELLER_AID] = ACCOUNT_INFO[playerid][ac_ID];
