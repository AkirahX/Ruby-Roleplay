CMD:sinalizador(playerid, params[])
{
    GetPlayerPos(playerid, CHARACTER_INFO[playerid][ch_POS][0], CHARACTER_INFO[playerid][ch_POS][1], CHARACTER_INFO[playerid][ch_POS][2]);
    CreateObject(18728, CHARACTER_INFO[playerid][ch_POS][0], CHARACTER_INFO[playerid][ch_POS][1] + 1, CHARACTER_INFO[playerid][ch_POS][2] - 2.4, 0.0, 0.0, 0.0, 1000.0);
