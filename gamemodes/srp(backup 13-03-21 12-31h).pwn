CMD:comprarcasa(playerid, params[])
{
	if(PLAYER_TEMP[playerid][pt_INTERIOR_INDEX] == -1) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	if(ENTER_EXIT[ PLAYER_TEMP[playerid][pt_INTERIOR_INDEX] ][ee_INTERIOR_TYPE] == INTERIOR_NO_INFO) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);

	
	new site_index = GetBuyPropertySiteIndexByIntTyp(ENTER_EXIT[ PLAYER_TEMP[playerid][pt_INTERIOR_INDEX] ][ee_INTERIOR_TYPE]);
	if(site_index == -1) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);

	if(!IsPlayerInRangeOfPoint(playerid, 1.0, BUY_PROPERTIES_SITES[site_index][site_X], BUY_PROPERTIES_SITES[site_index][site_Y], BUY_PROPERTIES_SITES[site_index][site_Z])) return ShowPlayerMessage(playerid, "Lugar incorreto.", 3);
	
	if(BANK_ACCOUNT[playerid][bank_account_ID] == 0) return SendClientMessage(playerid, -1, "{999999}Você não pode comprar uma casa sem conta bancária.");

	if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, -1, "Error: {C4FF66}/comprarcasa {FFFFFF}[ID]");
	if(params[0] <= 0) return SendClientMessage(playerid, -1, "{999999}Erro: ID inválido.");
	
	new DBResult:Result, DB_Query[120], player_properties;
	format(DB_Query, sizeof(DB_Query), "SELECT COUNT(`ID_USER`) AS `PROPERTIES` FROM `PROPERTY_OWNER` WHERE `ID_USER` = '%d';", ACCOUNT_INFO[playerid][ac_ID]);
	Result = db_query(Database, DB_Query);
	if(db_num_rows(Result)) player_properties = db_get_field_assoc_int(Result, "PROPERTIES");
	db_free_result(Result);
	
	if(ACCOUNT_INFO[playerid][ac_SU])
	{
		if(player_properties >= MAX_SU_PROPERTIES) return SendClientMessage(playerid, -1, "{999999}Você não pode comprar mais propriedades.");
	}
	else
	{
		if(player_properties >= MAX_NU_PROPERTIES)
		{
			SendClientMessageEx(playerid, -1, "{CCCCCC}¡Os jogadores {b1fc6f}VIP {CCCCCC}podem ter até %d propriedades! Use {fffb49}/ajuda {CCCCCC}se deseja ser {b1fc6f}VIP.", MAX_SU_PROPERTIES);
			SendClientMessage(playerid, -1, "{999999}Você não pode comprar mais propriedades.");
		}
	}
	
	new index = GetPropertyIndexByID(params[0]);
	if(index == -1) return SendClientMessage(playerid, -1, "{999999}Erro: ID inválido.");
	
	if(PROPERTY_INFO[index][property_SOLD]) return SendClientMessage(playerid, -1, "{999999}Esta propriedade já foi comprada.");
	if(PROPERTY_INFO[index][property_LEVEL] > ACCOUNT_INFO[playerid][ac_LEVEL]) return SendClientMessageEx(playerid, -1, "{999999}Deve ser no minimo nível %d para comprar esta propriedade.", PROPERTY_INFO[index][property_LEVEL]);
	if(PROPERTY_INFO[index][property_VIP_LEVEL] > ACCOUNT_INFO[playerid][ac_SU]) return SendClientMessageEx(playerid, -1, "{999999}É necessário filiação VIP%d para comprar esta propriedade.", PROPERTY_INFO[index][property_VIP_LEVEL]);
	if(PROPERTY_INFO[index][property_EXTRA] > ACCOUNT_INFO[playerid][ac_SD]) return SendClientMessageEx(playerid, -1, "{999999}NEcessário %d "SERVER_COIN" para comprar esta propriedade.", PROPERTY_INFO[index][property_EXTRA]);
	if(BANK_ACCOUNT[playerid][bank_account_BALANCE] >= PROPERTY_INFO[index][property_PRICE])
	{
		PLAYER_TEMP[playerid][pt_BUY_HOUSE_INDEX] = index;
		ShowDialog(playerid, DIALOG_CONFIRM_BUY_PROPERTY);
	}
	else SendClientMessageEx(playerid, -1, "{999999}Não tem dinheiro suficiente em sua conta bancária, faltam {FF6633}%s$ {999999}para poder comprar esta propriedade.", number_format_thousand(PROPERTY_INFO[index][property_PRICE] - BANK_ACCOUNT[playerid][bank_account_BALANCE]));
	return 1;
}

GetPropertyIndexByID(id)
{
	for(new i = 0; i != MAX_PROPERTIES; i ++)
	{
		if(!PROPERTY_INFO[i][property_VALID]) continue;
		if(PROPERTY_INFO[i][property_ID] == id) return i;
	}
	return -1;
}

