			work_extra_payment = (work_info[WORK_TAXI][work_info_EXTRA_PAY] * floatround(floatdiv(PLAYER_SKILLS[ driver ][WORK_TAXI], work_info[WORK_TAXI][work_info_EXTRA_PAY_EXP])));
