CMD:barreiras(playerid, params[])
{
	if(!PLAYER_WORKS[playerid][WORK_POLICE]) return SendClientMessage(playerid, -1, "{999999}Você não é policial.");
	if(PLAYER_TEMP[playerid][pt_WORKING_IN] != WORK_POLICE) return SendClientMessage(playerid, -1, "{999999}Você não está em serviço policial.");
	if(PLAYER_SKILLS[playerid][WORK_POLICE] < 8) return SendClientMessageEx(playerid, 0xFFA97FFF, "Deve ter no minimo o cargo %s para colocar objetos policiais.", POLICE_RANKS[8]);
	
	if(GetPlayerVirtualWorld(playerid) != 0 || GetPlayerInterior(playerid) != 0) return SendClientMessage(playerid, -1, "{999999}Não pode colocar objetos policiais aqui.");
	
	new type;
	if(sscanf(params, "d", type)) return SendClientMessage(playerid, -1, "Erro: {C4FF66}/barreira {FFFFFF}[1-5]");
	
	new modelid;
	switch(type)
	{
		case 1: modelid = 1459;
		case 2: modelid = 1238;
		case 3: modelid = 1425;
		case 4: modelid = 3265;
		case 5: modelid = 3091;
		default: return SendClientMessage(playerid, -1, "Objeto inválido.");
	}
	
	new index = GetFreePoliceObjectSlot();
	if(index == -1) return SendClientMessage(playerid, -1, "Não pode criar mais objetos policiais.");
	
	POLICE_OBJECTS[index][police_object_VALID] = true;
	format(POLICE_OBJECTS[index][police_object_USER], 24, "%s", ACCOUNT_INFO[playerid][ac_NAME]);
	
	new Float:pos[3], Float:angle;
	GetPlayerPos(playerid, pos[0], pos[1], pos[2]);
	GetPlayerFacingAngle(playerid, angle);
	pos[0] += (1.5 * floatsin(-angle, degrees));
	pos[1] += (1.5 * floatcos(-angle, degrees));
	
	POLICE_OBJECTS[index][police_object_OBJECT_ID] = CreateDynamicObject(modelid, pos[0], pos[1], pos[2]-0.5, 0.0, 0.0, angle, 0, 0);
	Streamer_Update(playerid);
	new info[2];
	info[0] = WORK_POLICE;
	info[1] = index; // Index
	Streamer_SetArrayData(STREAMER_TYPE_OBJECT, POLICE_OBJECTS[index][police_object_OBJECT_ID], E_STREAMER_EXTRA_ID, info);
	PLAYER_TEMP[playerid][pt_SELECTED_POLICE_OBJECT_INDEX] = index;
	EditDynamicObject(playerid, POLICE_OBJECTS[index][police_object_OBJECT_ID]);
	
	SendClientMessage(playerid, -1, "Coloque o objeto, use /ebarreiras para editar ou apagar.");
	return 1;
}

